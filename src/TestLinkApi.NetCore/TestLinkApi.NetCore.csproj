<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>TestLink .NET Core</Title>
    <TargetFramework>net5.0</TargetFramework>
    <RootNamespace>TestLinkApi</RootNamespace>
    <Authors>mastertnt</Authors>
    <Company></Company>
    <Description>Testlink Api for .NET Core

Forked from : https://github.com/freemanke/testlinkapi.netcore</Description>
    <Copyright>Freeman Ke / mastertnt</Copyright>
